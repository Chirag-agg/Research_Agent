# Deep Research Agent - Environment Variables
# Copy this file to .env and fill in your API keys

# LLM Provider Keys (Choose one or more)
OPENROUTER_API_KEY=sk-or-v1-...
GEMINI_API_KEY=your_gemini_api_key_here
TOGETHER_API_KEY=your_together_api_key_here
CEREBRAS_API_KEY=your_cerebras_api_key_here

# Search APIs (choose one or both)
EXA_API_KEY=your_exa_api_key_here
TAVILY_API_KEY=your_tavily_api_key_here

# Web Scraping
FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# Kestra Configuration
KESTRA_URL=http://localhost:8080
KESTRA_API_TOKEN=

# Model Configuration
DEFAULT_MODEL=meta-llama/Llama-3.1-70B-Instruct-Turbo
FAST_MODEL=meta-llama/Llama-3.1-8B-Instruct-Turbo
# For Cerebras, supported chat models: gpt-oss-120b (default/reasoning), llama3.1-8b (fast)

# V2 Storage Configuration (Supabase)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_api_key

# V2 Memory Configuration (Mem0)
MEM0_API_KEY=your_mem0_api_key
MEM0_ORG_ID=your_mem0_org_id
MEM0_PROJECT_ID=your_mem0_project_id
